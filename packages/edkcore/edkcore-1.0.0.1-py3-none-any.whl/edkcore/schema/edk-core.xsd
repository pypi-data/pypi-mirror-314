<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="http://www.edkcore.org/XMLSchema"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://www.edkcore.org/XMLSchema"
           elementFormDefault="qualified">
    <xs:include schemaLocation="meta.xsd"/>
    <xs:include schemaLocation="contextdata.xsd"/>
    <xs:element name="EDK" >
        <xs:complexType>
            <xs:all>
                <xs:element ref="MetaAction" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="AssertAction" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="SceneAction" minOccurs="0" maxOccurs="1" />
                <xs:element ref="ContextData" minOccurs="0" maxOccurs="1"/>
            </xs:all>
        </xs:complexType>
    </xs:element>


</xs:schema>