<?xml version="1.0" encoding="utf-8" ?>
<EDK xmlns="http://www.edkcore.org/XMLSchema"
     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xsi:schemaLocation="http://www.edkcore.org/XMLSchema edk-core.xsd">
<SceneAction name="TestScene">
	<Properties>
		<Property name="Description">创建新的受试者</Property>
    </Properties>
    <ACS>
	    <MetaAction class="m1" contextData="cd1">
            <Properties>
	            <Property name="Description">进入Study页面获取Site所有信息</Property>
            </Properties>
        </MetaAction>
        <MetaAction class="m2" contextData="cd2">
            <Properties>
	            <Property name="Description">根据指定的SiteCode获取系统生成的受试者名</Property>
            </Properties>
        </MetaAction>
        <MetaAction class="m3" contextData="cd3">
            <Properties>
            <Property name="Description">根据系统自动生成的受试者名,创建受试者</Property>
            </Properties>
        </MetaAction>
    </ACS>
</SceneAction>
</EDK>